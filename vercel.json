{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico",
            "headers": {
                "cache-control": "public, max-age=31536000"
            }
        },
        {
            "src": "/manifest.json",
            "dest": "/manifest.json",
            "headers": {
                "cache-control": "public, max-age=31536000"
            }
        },
        {
            "src": "/robots.txt",
            "dest": "/robots.txt",
            "headers": {
                "cache-control": "public, max-age=31536000"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html",
            "headers": {
                "cache-control": "public, max-age=0, must-revalidate"
            }
        }
    ],
    "functions": {
        "src/pages/api/**/*.js": {
            "runtime": "nodejs18.x"
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}